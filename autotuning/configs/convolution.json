{
  "application_name" : "rise",
  "optimization_objectives" : ["runtime"],
  "hypermapper_mode" : {
    "mode" : "client-server"
  },
  "feasible_output" : {
    "enable_feasible_predictor" : true,
    "name" : "Valid",
    "true_value" : "True",
    "false_value" : "False"
  },
  "design_of_experiment" : {
    "doe_type" : "random sampling",
    "number_of_samples" : 1
  },
  "optimization_iterations" : 100,
  "local_search_random_points": 100,
  "epsilon_greedy_threshold": 1,
  "input_parameters" : {
    "ls0" : {
      "parameter_type" : "integer",
      "values" : [1, 1024],
      "constraints" : ["gs0 % ls0 == 0", "ls0 <= gs0"],
      "dependencies" : ["gs0"]
    },
    "tile" : {
      "parameter_type" : "integer",
      "values" : [1, 128],
      "constraints" : ["tile % vec == 0", "tile >= vec", "((-1 * vec * vec)+(3*vec) + (tile*vec)) >= 0", "(3+tile+(-1*vec))>=0", "tile/vec >= 0"],
      "dependencies" : ["vec"]
    },
    "ls1" : {
      "parameter_type" : "integer",
      "values" : [1, 1024],
      "constraints" : ["gs1 % ls1 == 0", "ls1 <= gs1"],
      "dependencies" : ["gs1"]
    },
    "vec" : {
      "parameter_type" : "integer",
      "values" : [2, 16],
      "constraints" : ["16 % vec == 0"],
      "dependencies" : []
    },
    "gs1" : {
      "parameter_type" : "integer",
      "values" : [1, 1024],
      "constraints" : ["1024 % gs1 == 0"],
      "dependencies" : []
    },
    "gs0" : {
      "parameter_type" : "integer",
      "values" : [1, 1024],
      "constraints" : ["1024 % gs0 == 0"],
      "dependencies" : []
    }
  }
}